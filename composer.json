{
    "name": "kucasoft/webulator",
    "description": "Simple vendor based web app",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "dejan-babic",
            "email": "babicaja@gmail.com"
        }
    ],
    "require": {
        "symfony/dotenv": "^4.1",
        "php-di/php-di": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kucasoft\\": "app/Kucasoft"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    }
}
