{
    "name": "kucasoft/webulator",
    "description": "Simple vendor based web app",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dejan Babic",
            "email": "dejan.babic@kucasoft.com"
        }
    ],
    "require": {
        "symfony/dotenv": "^4.1",
        "php-di/php-di": "^6.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "zendframework/zend-diactoros": "^1.8",
        "nikic/fast-route": "^1.3",
        "myclabs/php-enum": "^1.6",
        "hassankhan/config": "1.1.0",
        "twig/twig": "2.x-dev",
        "mikey179/vfsstream": "2.0.x-dev",
        "monolog/monolog": "^1.23",
        "usmanhalalit/pixie": "^2.0",
        "guzzlehttp/guzzle": "6.5.8",
        "zendframework/zend-httphandlerrunner": "^1.0",
        "ext-xdebug": "^2.6",
        "umpirsky/twig-php-function": "^0.2.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Webulator\\": "app/Webulator"
        },
        "files": [
            "app/Webulator/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    }
}
